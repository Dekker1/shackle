<instance format="XCSP3" type="QSTR">
  <variables>
    <var id="x" type="point"/>
    <var id="y" type="point"/>
    <var id="z" type="point"/>
  </variables>   
  <constraints>
    <point>
      <scope> x y </scope>
      <relation> b a </relation>
    </point>
    <point>
      <scope> x z </scope>
      <relation> b eq </relation>
    </point>
    <point>
      <scope> y z </scope>
      <relation> a </relation>
    </point>
  </constraints>
</instance>
